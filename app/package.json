{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:fast": "vite dev --force --port 5173",
		"dev:open": "vite dev --open",
		"dev:host": "vite dev --host",
		"dev:https": "vite dev --https",
		"dev:full": "vite dev --host --https --open",
		"build": "vite build",
		"build:preview": "npm run build && npm run preview",
		"preview": "vite preview",
		"preview:host": "vite preview --host",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npm run check",
		"clean": "rm -rf node_modules .svelte-kit build dist",
		"clean:install": "npm run clean && npm install",
		"test:build": "npm run build && npm run preview",
		"test:build:host": "npm run build && npm run preview:host",
		"deploy:vercel": "vercel --prod",
		"deploy:netlify": "npm run build && npx netlify-cli deploy --prod --dir=build",
		"deploy:github": "npm run build && gh-pages -d build",
		"deploy:render": "npm run build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.9",
		"ogl": "^1.0.11",
		"vite-plugin-pwa": "^1.0.2"
	}
}
